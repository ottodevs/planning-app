{
  "name": "app-name",
  "version": "1.0.0",
  "description": "",
  "dependencies": {
    "@aragon/apps-token-manager": "^1.0.0",
    "@aragon/apps-voting": "^1.0.0",
    "@aragon/react-scripts": "^1.1.0-aragon.4",
    "@aragon/os": "3.1.9",
    "@aragon/client": "^1.0.0-beta.9",
    "@aragon/ui": "^0.15.0",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "styled-components": "^3.2.6"
  },
  "devDependencies": {
    "@aragon/cli": "^4.0.6",
    "babel-plugin-styled-components": "^1.5.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-2": "^6.24.1",
    "parcel-bundler": "^1.8.1",
    "truffle": "4.0.5"
  },
  "scripts": {
    "start": "aragon run && cd app/  && react-scripts start",
    "start:kit": "aragon run --kit Kit --kit-init @ARAGON_ENS && react-scripts start",
    "test": "aragon contracts test",
    "compile": "aragon contracts compile",
    "sync-assets": "mkdir -p public; rsync -rtu \"$(dirname $(node -p 'require.resolve(\"@aragon/ui\")'))/\" public/aragon-ui",
    "build:app": "parcel build app/index.html -d public/ --public-url '.'",
    "build:script": "exit 0 && parcel build app/script.js -d public/",
    "build": "npm run sync-assets && npm run build:app && npm run build:script",
    "publish": "aragon publish"
  },
