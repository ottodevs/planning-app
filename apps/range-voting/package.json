{
  "name": "@tps/apps-range-voting",
  "version": "0.0.1",
  "description": "Range Voting app from Planning Suite",
  "scripts": {
    "build:app": "parcel build app/index.html -d dist/ --public-url '.'",
    "build:script": "parcel build app/script.js -d dist/",
    "build": "npm run compile && npm run sync-assets && npm run build:app && npm run build:script",
    "compile": "truffle compile",
    "console": "truffle dev",
    "coverage": "SOLIDITY_COVERAGE=true npm run ganache-cli:test",
    "dev": "npm run sync-assets && parcel app/index.html --port 4444",
    "ganache-cli:test": "../shared/test-helpers/ganache-cli.sh",
    "lint": "solium --dir ./contracts",
    "prepublishOnly": "truffle compile --all",
    "publish:http": "npm run publish:http-copy-files && aragon apm publish patch --http localhost:4444 --http-served-from ./dist",
    "publish:http-copy-files": "npm run build:script && mkdir -p dist/images && cp images/icon.svg dist/images/",
    "publish": "aragon apm publish patch && sleep 3",
    "publish:major": "aragon apm publish major",
    "deploy:rinkeby": "npm run publish:major -- --environment staging",
    "start": "aragon run",
    "sync-assets": "copy-aragon-ui-assets ./dist",
    "test:gas": "GAS_REPORTER=true npm test",
    "test": "TRUFFLE_TEST=true npm run ganache-cli:test"
  },
  "dependencies": {
    "@aragon/client": "1.1.0",
    "@aragon/ui": "0.30.1",
    "@babel/polyfill": "^7.2.5",
    "bignumber.js": "^7.2.1",
    "prop-types": "15.7.2",
    "react": "^16.8.2",
    "react-blockies": "1.3.0",
    "react-dom": "^16.8.2",
    "react-motion": "0.5.2",
    "styled-components": "4.1.3"
  },
  "devDependencies": {
    "@babel/core": "^7.3.3",
    "@babel/plugin-proposal-class-properties": "^7.3.3",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "@tps/test-helpers": "^0.0.1",
    "babel-eslint": "^10.0.1",
    "babel-plugin-styled-components": "^1.10.0",
    "eslint": "^5.14.1",
    "eslint-config-prettier": "^4.0.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-config-standard-react": "^7.0.2",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-standard": "^4.0.0",
    "parcel-bundler": "^1.11.0",
    "prettier": "^1.16.4",
    "stylelint": "^9.10.1",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.5.2"
  },
  "author": "",
  "repository": {
    "type": "git",
    "url": "https://github.com/AutarkLabs/planning-suite"
  },
  "keywords": [],
  "license": "GPL-3.0"
}
