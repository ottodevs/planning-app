{
  "name": "@tpt/apps-projects",
  "version": "0.0.1",
  "description": "Project management app from Planning Suite",
  "scripts": {
    "build:app": "parcel build app/index.html -d dist/ --public-url '.'",
    "build:script": "parcel build app/script.js -d dist/",
    "build": "npm run compile && npm run sync-assets && npm run build:app && npm run build:script",
    "compile": "truffle compile",
    "console": "truffle dev",
    "coverage": "SOLIDITY_COVERAGE=true npm run ganache-cli:test",
    "dev": "npm run sync-assets && parcel app/index.html --port 3333",
    "ganache-cli:test": "../../packages/test-helpers/ganache-cli.sh",
    "lint": "npm run lint:sol && npm run lint:js",
    "lint:sol": "solium --dir contracts",
    "lint:js": "eslint . --fix",
    "prepublishOnly": "truffle compile --all",
    "publish:http": "npm run publish:http-copy-files && aragon apm publish --http localhost:3333 --http-served-from ./dist",
    "publish:http-copy-files": "npm run build:script && mkdir -p dist/images && cp images/icon.svg dist/images/",
    "publish": "aragon apm publish",
    "start": "aragon run",
    "sync-assets": "copy-aragon-ui-assets ./dist",
    "test:e2e": "cypress open",
    "test:unit": "npx jest app/test/**/*.spec.js",
    "test:unit:watch": "npm run test:unit -- --watchAll",
    "test:gas": "GAS_REPORTER=true npm test",
    "test": "TRUFFLE_TEST=true npm run ganache-cli:test"
  },
  "dependencies": {
    "@aragon/client": "1.0.0-beta.9",
    "@aragon/ui": "^0.27.0",
    "graphql-request": "^1.8.2",
    "prop-types": "15.6.2",
    "react": "16.6.1",
    "react-dom": "16.6.1",
    "react-number-format": "3.5.2",
    "rxjs": "6.3.2",
    "rxjs-compat": "6.3.2",
    "styled-components": "3.4.6"
  },
  "devDependencies": {
    "@tpt/test-helpers": "^0.0.1",
    "babel-core": "6.26.3",
    "babel-eslint": "9.0.0",
    "babel-jest": "^23.6.0",
    "babel-plugin-styled-components": "1.5.1",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-env": "1.7.0",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-3": "6.24.1",
    "cypress": "^3.1.1",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.7.0",
    "eslint": "5.5.0",
    "eslint-config-prettier": "3.0.1",
    "eslint-plugin-cypress": "^2.1.2",
    "eslint-plugin-jest": "^22.0.0",
    "eslint-plugin-react": "7.11.1",
    "jest": "^23.6.0",
    "parcel-bundler": "1.9.7",
    "prettier": "1.14.2",
    "react-hot-loader": "4.3.12",
    "solium": "1.1.8",
    "stylelint": "9.5.0",
    "stylelint-config-standard": "18.2.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-processor-styled-components": "1.3.2"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>/app/test/config/jest.setup.js",
    "moduleNameMapper": {
      "\\.(svg)$": "<rootDir>/app/test/config/assetsTransformer.js"
    }
  },
  "author": "",
  "repository": {
    "type": "git",
    "url": "https://github.com/Giveth/planning-app"
  },
  "keywords": [],
  "license": "GPL-3.0"
}
